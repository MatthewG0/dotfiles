// Place your settings in this file to overwrite the default settings
{
  "editor.tabSize": 2,
  "editor.minimap.enabled": true,
  "eslint.run": "onType",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules/**": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Visual Studio Light",
  "workbench.iconTheme": "vs-seti",
  "window.nativeTabs": true,
  "ruby.lint": {
    "rubocop": true
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "python.linting.pylintPath": "pylint",
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "npm.enableScriptExplorer": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "eslint.options": {
    "ignorePattern": "**/node_modules/** **/build/**"
  },
  "workbench.colorCustomizations": {
    "terminal.background": "#000000",
    "terminal.foreground": "#dadada",
    "terminal.selectionBackground": "#00eeff83"
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vs-kubernetes": {
    "vs-kubernetes.draft-path": "/Users/matthew.goldberg/.vs-kubernetes/tools/draft/darwin-amd64/draft",
    "vs-kubernetes.helm-path": "/Users/matthew.goldberg/.vs-kubernetes/tools/helm/darwin-amd64/helm",
    "vs-kubernetes.minikube-path": "/Users/matthew.goldberg/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "gitlens.views.compare.files.layout": "tree",
  "editor.suggestSelection": "first",
  "extensions.ignoreRecommendations": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "terminal.integrated.shell.osx": "/bin/zsh",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.scrollback": 9999,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
